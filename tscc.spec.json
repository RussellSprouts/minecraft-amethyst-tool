{
    "modules": {
        "root": "src/root.ts",
        "renderer": {
            "entry": "src/renderer.ts",
            "dependencies": [
                "root"
            ]
        },
        "main": {
            "entry": "src/main.ts",
            "dependencies": [
                "renderer"
            ]
        },
        "geode": {
            "entry": "src/geode.ts",
            "dependencies": [
                "renderer"
            ]
        },
        "worker": {
            "entry": "src/worker.ts",
            "dependencies": [
                "root"
            ]
        }
    },
    "external": {
        "pako": "pako",
        "three": "THREE",
        "three/examples/jsm/controls/OrbitControls": "OrbitControls"
    },
    "compilerFlags": {
        "language_out": "ECMASCRIPT_2020",
        "create_source_map": "%outname%.map",
        "emit_use_strict": true
    },
    "prefix": "out/"
}